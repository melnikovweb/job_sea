<section class="{{ class }} {{ classes }} {{ block_classes }}" {{ block.anchor ? 'id=' ~ block.anchor : null }}>



  {% if not (title is empty) %}
  <h2 class="{{ class }}-title">{{ title }}</h2>
{% endif %}

  <div class="{{ class }}-container sk-container--fl">
 
    {% if t_icon %}
    <div class="{{ class }}-top-icon {{ class }}-only-d">
      {% include 'parts/components/image.twig' with {'id': t_icon, 'class': 'img-fluid', 'sizes': '329px'} only %}
    </div>
    {% endif %}

    {% if bg_image_desktop %}
    <div class="{{ class }}-bg-image {{ class }}-only-d">
      {% include 'parts/components/image.twig' with {'id': bg_image_desktop, 'class': 'img-fluid', 'sizes': '748px'} only %}
    </div>
    {% endif %}

    {% if b_icon %}
    <div class="{{ class }}-bottom-icon {{ class }}-only-d">
      {% include 'parts/components/image.twig' with {'id': b_icon, 'class': 'img-fluid', 'sizes': '329px'} only %}
    </div>
    {% endif %}


    <ul class="{{ class }}-items">
    {% for item in items %}

    <li>
      <div class="{{ class }}-item">

      {% if item.image %}
      <div class="{{ class }}-item-image" style="background-color:{{item.color}}">
        {% include 'parts/components/image.twig' with {'id': item.image, 'class': 'img-fluid', 'sizes': '90px'} only %}
      </div>
      {% endif %}

        {% if not (item.description is empty) %}
        <div class="{{ class }}-item-description">{{ item.description }}</div>
        {% endif %}
      
      </div>
    </li>
    {% endfor %}
  </ul>










  </div>
</section>


<section class="{{ cta_class }} {{ classes }} {{ block_classes }}" {{ block.anchor ? 'id=' ~ block.anchor : null }}>
  <div class="sk-container sk-container--fl">

    <div class="{{ cta_class }}-content">

      {% if bg_image_mobile %}
      <div class="{{ class }}-bg-image {{ class }}-image-mobile">
        {% include 'parts/components/image.twig' with {'id': bg_image_mobile, 'class': 'img-fluid', 'sizes': '748px'} only %}
      </div>
      {% endif %}

      {% if not (cta_title is empty) %}
        <h2 class="{{ cta_class }}-title">{{ cta_title }}</h2>
      {% endif %}

      {% if not (button is empty) %}
      <div class="{{ cta_class }}-buttons" >
        
    
            {% set target = button.target ? '_blank' : '_self' %}
            <a class="{{ cta_class }}-btn sk-btn" href="{{ button.url }}"
               target="{{ target }}">{{ button.title }}</a>
              
        </div>
        {% endif %}
    </div>

  </div>
</section>