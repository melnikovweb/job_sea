<section class="{{ class }} {{ classes }} {{ block_classes }}" id="{{ block.id }}">
  <div class="sk-container">

    <div class="{{ class }}-content">
      {% if not (fields.title is empty) %}
        <h2 class="{{ class }}-title">{{ fields.title }}</h2>
      {% endif %}

      {% if not (fields.members is empty) %}
        <div class="{{ class }}-members">
          {% for item in fields.members %}
            {% if not (item.logo is empty) %}
              <div class="{{ class }}-members-logo">
                {% if not (item.link is empty) %}
                <a href="{{ item.link }}" title="Members link" target="_blank">
                  {% endif %}
                  {% include 'parts/components/image.twig' with {'id': item.logo.id, 'classes': 'member-logo', 'sizes': '(max-width: 767px) 200px, (max-width: 991px) 400px, 600px', 'disableLoading': false} only %}
                  {% if not (item.link is empty) %}
                </a>
                {% endif %}
              </div>
            {% endif %}
          {% endfor %}
        </div>
      {% endif %}

      {% if not (fields.info is empty) %}
        <div class="{{ class }}-info">{{ fields.info }}</div>
      {% endif %}

      {% if not (fields.button is empty) %}
        {% set button =  fields.button %}
        <div class="{{ class }}-button">
          <a href="{{ button.url }}" target="{{ button.target ? button.target : '_self' }}"
             title="{{ button.title ? button.title : button.url }}"
             class="sk-btn sk-btn--primary">
            {{ button.title ? button.title : button.url }}
          </a>
        </div>
      {% endif %}
    </div>

  </div>
</section>
